  d1$geographic_region <- fct_collapse(d1$geographic_region, Geographic_Africa = c("Algeria", "Angola", "Benin", "Botswana", 
                                                                                   "Burkina Faso", 
                                                          "Burundi", "Cabo Verde", "Cameroon", "Central African Republic", 
