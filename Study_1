                 response = model_csf_glm_k_fold_covariates$pred$obs, 
                 predictor = model_csf_glm_k_fold_covariates$pred$BIO_AD_Positive,
                 levels = rev(levels(model_csf_glm_k_fold_covariates$pred$obs)))
