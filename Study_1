################################################################# Plot to display prevalence rates... ################################################################
```{r}
library(ggplot2)
library(arsenal)
d1_no_NA <- subset(d1, geographic_region != "NA")
d1_csf_no_NA <- subset(d1_csf, geographic_region != "NA")

ggplot(d1_no_NA, aes(x= interaction(geographic_region, AD_clinical_status), y = raw_Ptau217_Lilly, color = AD_clinical_status)) + 
  geom_boxplot(outlier.shape = NA, alpha = 0.5) +
    scale_fill_manual(values = c("#E69F00", "#56B4E9")) +
  scale_color_manual(values = c("#E69F00", "#56B4E9")) +
  geom_jitter(position = position_jitter(width = 0.2), size = 1, alpha = 0.7) +
  labs(x = "Ethnicity and AD status", y = "Plasma Ptau217", title = "Plasma ptau217 by ethnicity (Full cohort)") + 
  scale_x_discrete(labels = c("DK.CLI_AD_negative" = "DK: Aβ-", "DK.CLI_AD_positive" = "DK: Aβ+", 
                              "Africa.CLI_AD_negative" = "Africa: Aβ-", "Africa.CLI_AD_positive" = "Africa: Aβ+", 
                              "Europe.CLI_AD_negative" = "Europe: Aβ-", "Europe.CLI_AD_positive" = "Europe: Aβ+",
                              "Mid_east.CLI_AD_negative" = "Middle East: Aβ-", "Mid_east.CLI_AD_positive" = "Middle East: Aβ+"))+
  theme_minimal()+
  theme(legend.position = "none")  


ggplot(d1_csf_no_NA, aes(x= interaction(geographic_region, AD_biomarker_status), y = raw_Ptau217_Lilly, color = AD_biomarker_status)) + 
  geom_boxplot(outlier.shape = NA, alpha = 0.5) +
      scale_fill_manual(values = c("#E69F00", "#56B4E9")) +
  scale_color_manual(values = c("#E69F00", "#56B4E9")) +
  geom_jitter(position = position_jitter(width = 0.2), size = 1, alpha = 0.7) +
  labs(x = "Ethnicity and AD status", y = "Plasma Ptau217", title = "Plasma ptau217 by ethnicity (CSF/Aβ-PET)") + 
  scale_x_discrete(labels = c("DK.BIO_AD_Negative" = "DK: Aβ-", "DK.BIO_AD_Positive" = "DK: Aβ+", 
                              "Africa.BIO_AD_Negative" = "Africa: Aβ-", "Africa.BIO_AD_Positive" = "Africa: Aβ+", 
                              "Europe.BIO_AD_Negative" = "Europe: Aβ-", "Europe.BIO_AD_Positive" = "Europe: Aβ+",
                              "Mid_east.BIO_AD_Negative" = "Middle East: Aβ-", "Mid_east.BIO_AD_Positive" = "Middle East: Aβ+"))+
  theme_minimal()+
  theme(legend.position = "none")  
  theme_minimal()


  
#Comparative statistics
CLI_AB_NEG <- subset(d1, AD_clinical_status == "CLI_AD_negative")
CLI_AB_POS <- subset(d1, AD_clinical_status == "CLI_AD_positive")
BIO_AB_NEG <- subset(d1_csf, AD_biomarker_status == "BIO_AD_Negative")
BIO_AB_POS <- subset(d1_csf, AD_biomarker_status == "BIO_AD_Positive")
summary(tableby(geographic_region ~ raw_Ptau217_Lilly, data=CLI_AB_NEG, control = median_settings))
summary(tableby(geographic_region ~ raw_Ptau217_Lilly, data=CLI_AB_POS, control = median_settings))
summary(tableby(geographic_region ~ raw_Ptau217_Lilly, data=BIO_AB_NEG, control = median_settings))
summary(tableby(geographic_region ~ raw_Ptau217_Lilly, data=BIO_AB_POS, control = median_settings))
```
